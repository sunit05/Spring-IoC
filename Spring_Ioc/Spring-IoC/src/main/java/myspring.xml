<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean id="MyEmployee" class="com.jsp.temp.Employee">
	<property name="id" value="123"></property>
	<property name="name" value="sunit"></property>
	<property name="mail" value="sunitmail"></property>	
	</bean>
	
	<bean id="MyStudent" class="com.jsp.student.constructorinjection.Student">
	<constructor-arg index="0" value="123"></constructor-arg>
	<constructor-arg index="1" value="sunit"></constructor-arg>
	<constructor-arg index="2" value="sunitmail"></constructor-arg>
	
	</bean>
</beans>
